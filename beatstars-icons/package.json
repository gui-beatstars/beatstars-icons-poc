{
  "name": "@beatstars/icons",
  "version": "1.0.0",
  "description": "BeatStars Icon Library â€” auto-generated from Figma",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "svg"
  ],
  "scripts": {
    "fetch": "node scripts/fetch-from-figma.mjs",
    "optimize": "node scripts/optimize-svgs.mjs",
    "generate": "node scripts/generate-components.mjs",
    "build:icons": "npm run fetch && npm run optimize && npm run generate",
    "build:lib": "node scripts/build-library.mjs",
    "build:catalog-data": "node scripts/build-catalog-data.mjs",
    "build": "npm run build:icons && npm run build:lib && npm run build:catalog-data",
    "dev:catalog": "npx serve web -l 3333",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "esbuild": "^0.20.0",
    "svgo": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "icons",
    "beatstars",
    "react",
    "svg",
    "design-system"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/beatstars/icons"
